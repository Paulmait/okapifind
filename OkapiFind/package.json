{
  "name": "okapifind",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "postinstall": "patch-package",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --output-dir dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__/(services|hooks|utils)",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:components": "jest --testPathPattern=__tests__/(screens|components)",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:update-snapshots": "jest --updateSnapshot",
    "coverage:open": "open coverage/lcov-report/index.html",
    "coverage:serve": "npx http-server coverage/lcov-report -p 8080",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clear",
    "eas:dev": "eas build --profile development --platform all",
    "eas:preview": "eas build --profile preview --platform all",
    "eas:staging": "eas build --profile staging --platform all",
    "eas:prod": "eas build --profile production --platform all",
    "eas:submit:preview": "eas submit --profile preview --platform all",
    "eas:submit:staging": "eas submit --profile staging --platform all",
    "eas:submit:prod": "eas submit --profile production --platform all",
    "eas:update:dev": "eas update --branch development --message",
    "eas:update:preview": "eas update --branch preview --message",
    "eas:update:prod": "eas update --branch production --message",
    "deploy:preview": "npm run test:ci && npm run typecheck && npm run lint && eas build --profile preview --platform all --auto-submit",
    "deploy:staging": "npm run test:ci && npm run typecheck && npm run lint && eas build --profile staging --platform all --auto-submit",
    "deploy:production": "npm run test:ci && npm run typecheck && npm run lint && eas build --profile production --platform all --auto-submit",
    "verify": "npm run typecheck && npm run lint && npm run test:ci",
    "clean": "rm -rf node_modules .expo dist build ios android",
    "clean:install": "npm run clean && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@apollo/client": "^4.0.5",
    "@google/generative-ai": "^0.24.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.3.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@sentry/react-native": "~7.2.0",
    "@supabase/supabase-js": "^2.58.0",
    "@types/node": "^24.5.2",
    "@types/redis": "^4.0.10",
    "@vercel/node": "^5.3.24",
    "apollo-server-express": "^3.13.0",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "eventemitter3": "^5.0.1",
    "expo": "~54.0.31",
    "expo-apple-authentication": "~8.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-av": "^16.0.7",
    "expo-build-properties": "~1.0.10",
    "expo-camera": "^17.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "^15.0.7",
    "expo-device": "^8.0.10",
    "expo-haptics": "^15.0.7",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "~17.0.10",
    "expo-intent-launcher": "^13.0.7",
    "expo-linear-gradient": "^15.0.1",
    "expo-location": "^19.0.7",
    "expo-maps": "~0.12.10",
    "expo-notifications": "~0.32.16",
    "expo-secure-store": "^15.0.7",
    "expo-sensors": "^15.0.7",
    "expo-speech": "^14.0.1",
    "expo-status-bar": "~3.0.8",
    "expo-store-review": "^9.0.7",
    "expo-task-manager": "^14.0.7",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.7",
    "express-rate-limit": "^8.1.0",
    "firebase": "^12.3.0",
    "graphql": "^16.11.0",
    "i18next": "^25.5.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-purchases": "^9.5.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-tts": "^4.1.1",
    "react-native-web": "^0.21.0",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.1.1",
    "speakeasy": "^2.0.0",
    "type-graphql": "^2.0.0-rc.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/cors": "^2.8.19",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/react": "~19.1.0",
    "@types/speakeasy": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-jest": "^30.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^20.20.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "^19.1.0",
    "sharp": "^0.34.4",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "exclude": []
    }
  }
}
