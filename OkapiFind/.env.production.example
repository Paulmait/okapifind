# Production Environment Configuration
# Copy this file to .env.production and fill in your production values

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your-production-firebase-api-key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-production.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-production-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-production.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-production-sender-id
EXPO_PUBLIC_FIREBASE_APP_ID=your-production-app-id
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=your-production-measurement-id

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your-production-supabase-url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-production-supabase-anon-key

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-production-web-client.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your-production-ios-client.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your-production-android-client.apps.googleusercontent.com

# RevenueCat Configuration
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=your-production-revenuecat-ios-key
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=your-production-revenuecat-android-key

# Sentry Configuration
EXPO_PUBLIC_SENTRY_DSN=your-production-sentry-dsn
EXPO_PUBLIC_SENTRY_ENVIRONMENT=production
EXPO_PUBLIC_SENTRY_DEBUG=false

# EAS Configuration
EAS_PROJECT_ID=your-eas-project-id

# App Configuration
EXPO_PUBLIC_API_BASE_URL=https://api.okapifind.com
EXPO_PUBLIC_ENVIRONMENT=production
EXPO_PUBLIC_LOG_LEVEL=error
EXPO_PUBLIC_ENABLE_ANALYTICS=true